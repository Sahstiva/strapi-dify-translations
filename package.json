{
  "name": "strapi-dify-translations",
  "version": "1.0.12",
  "description": "Strapi plugin to translate content using Dify AI workflows",
  "license": "MIT",
  "author": "",
  "maintainers": [],
  "exports": {
    "./strapi-admin": {
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sahstiva/strapi-dify-translations"
  },
  "bugs": {
    "url": "https://github.com/Sahstiva/strapi-dify-translations/issues"
  },
  "homepage": "https://github.com/Sahstiva/strapi-dify-translations#readme",
  "keywords": ["strapi", "strapi-plugin", "translations", "i18n", "dify"],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.14",
    "@strapi/icons": "^2.0.0-rc.14"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.0.0",
    "@strapi/strapi": "^5.0.0",
    "@strapi/typescript-utils": "^5.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "prettier": "^3.3.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.22.0",
    "styled-components": "^6.1.0",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "strapi": {
    "kind": "plugin",
    "name": "dify-translations",
    "displayName": "Dify Translations",
    "description": "Translate content using Dify AI workflows"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}

